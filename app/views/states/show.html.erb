<p style="color: green"><%= notice %></p>

<div class="container-fluid">
  <div class="card">
  <%= render @state %>
  </div>
  <br>

  <%= link_to "Show", @state.attachment_url, class:"btn btn-primary"%>

  <%= link_to "Edit this state", edit_state_path(@state), class:"btn btn-success" %>
  
  <%= link_to "Back to states", states_path, class:"btn btn-warning" %>

  <%= button_to "Destroy this state", @state, method: :delete, class:"btn btn-danger" %>  
</div>
